#WFLAGS += -DMT7628_FPGA -DCONFIG_FPGA_MODE


HAS_MT_BBP=y
HAS_MT_MAC=y
HAS_INF_RBUS=y
HAS_MAC_PCI=y
HAS_MT_MAC=y
HAS_MT_RF=y
